generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model stories {
  id         Int      @id @default(autoincrement())
  title      String
  story_type String
  story      String   @db.Text
  latlong    String
  createdAt  DateTime @default(now())
  location   String

  @@map("stories")
}
